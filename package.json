{
    "name": "brutefarce",
    "version": "1.0.0",
    "description": "Bruteforce passwords in farcical way",
    "repository": {
      "type": "git",
      "url": "git+https://github.com/stscoundrel/brutefarce.git"
    },
    "author": "",
    "license": "MIT",  
    "scripts": {
      "build:c": "gcc -o brutefarce-c C/src/main.c",
      "run:c": "yarn build:c && ./brutefarce-c",
      "build:c++": "g++ -o brutefarce-cpp C++/src/main.cpp",
      "run:c++": "yarn build:c++ && ./brutefarce-cpp",
      "run:rust": "cd Rust && cargo run",
      "build:typescript": "cd TypeScript && yarn",
      "run:typescript": "yarn build:typescript && cd TypeScript && yarn start",
      "run:python": "python3 Python/src/main.py",
      "run:c#": "dotnet run --project C#/src",
      "run:nim": "nim c -r Nim/src/main.nim",
      "build:java": "javac ./Java/src/brutefarce/Main.java",
      "run:java": "yarn build:java && cd Java/src && java brutefarce.Main",
      "build:go": "cd Go/src && go build -o brutefarce-go main.go",
      "run:go": "yarn build:go && ./Go/src/brutefarce-go"
    }
  }
  